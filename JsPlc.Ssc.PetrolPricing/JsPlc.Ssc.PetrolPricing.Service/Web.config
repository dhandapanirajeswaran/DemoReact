	<nlog xmlns="http://www.nlog-project.org/schemas/NLog.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" throwExceptions="true">
		<variable name="log_dir" value="C:\log" />
		<variable name="archive_dir" value="${basedir}/Log/Archive" />
		<targets async="true">
			<target name="jsonFile" xsi:type="File" fileName="${log_dir}/planitapilog_${shortdate}.json">
				<layout xsi:type="JsonLayout">
					<attribute name="Source" layout="JsPlc.Planit.Service" />
					<attribute name="time" layout="${longdate}" />
					<attribute name="level" layout="${level:upperCase=true}" />
					<attribute name="message" layout="${message}" />
				</layout>
			</target>
		</targets>
		<rules>
			<logger name="*" minlevel="Info" writeTo="jsonFile" />
		</rules>
	</nlog>
